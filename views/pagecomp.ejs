<html>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
<style>
    i {
        border: 1px solid cadetblue;
        font-size: 20px;
        width: 30px;
        text-align: center;
        height: 20px;
        background-color: cadetblue;
        color: black;
        padding: 10px;
        border-radius: 10px;
    }

    div {
        text-align: end;
    }
</style>

<body>

    <%if(Number(p)!=1){ %>
        <a href="/sqlTableGen?textq=<%=Usersql%>&page=1"><i class="fa-solid fa-backward"></i></a>
        <a href="/sqlTableGen?textq=<%=Usersql%>&page=<%= Number(p)-1 %>"><i class="fa-solid fa-arrow-left"></i> </a>
    <% } %>
    <%=p%> Page
    <% if(Number(p) !=lastlimit) { %>
        <a href="/sqlTableGen?textq=<%=Usersql%>&page=<%= Number(p)+1 %>"><i class="fa-solid fa-arrow-right"></i> </a>
        <a href="/sqlTableGen?textq=<%=Usersql%>&page=<%= lastlimit %>"><i class="fa-solid fa-forward"></i> </a>
    <% } %>
</body>